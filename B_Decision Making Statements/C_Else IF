#include<stdio.h>
int main(){
  int a;
  printf("enter your age");
  scanf("%d",&a);
  if(a<=10){
    printf("%d is smaller than 10 or equal to 10",a);
  }
  else if (a<=18){
        printf("%d is smaller than 18 or equal to 18",a);
  }
  else if (a>=18){
        printf("%d is greater than 18 ",a);
  }
  return 0;
  }