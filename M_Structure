#include <stdio.h>
struct student{
char name[20];
int roll;
float salary;

}s;
void main(){
  printf("enter roll");
  scanf("%d",&s.roll);
    printf("enter name");
  scanf("%s",s.name);
    printf("enter salary");
  scanf("%f",&s.salary);
  printf("roll is %d\n",s.roll);
  printf("name is %s\n",s.name);
  printf("salary is %f\n",s.salary);
}